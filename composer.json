{
    "name": "fntneves/laravel-transactional-events",
    "description": "Transaction-aware Event Dispatcher for Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "loophp/phptree": "^2.5",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/events": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "authors": [
        {
            "name": "Francisco Neves",
            "email": "hey@francisconeves.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Neves\\": "src/"
        },
        "files": [
            "src/Neves/Events/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neves\\Events\\EventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0"
    }
}
