{
    "name": "fntneves/laravel-transactional-events",
    "description": "Transactional layer for Laravel Event Dispatcher",
    "type": "library",
    "license": "MIT",
    "require": {
        "loophp/phptree": "^2.5",
        "illuminate/database": "~5.8.0|^6.0|^7.0",
        "illuminate/events": "~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0"
    },
    "authors": [
        {
            "name": "Francisco Neves",
            "email": "hello@neves.info"
        }
    ],
    "autoload": {
        "psr-0": {
            "Neves\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neves\\Events\\EventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|~4.0|^5.0"
    }
}
